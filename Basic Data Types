//Code below uses stl concepts

#include <iostream>
#include <cstdio>
#include<string>
#include<sstream>
#include<stdio.h>
#include<iomanip>
using namespace std;

int main() {
    string input;
    int a;
    long b;
    char c;
    float f;
    double d;
    getline(cin, input);
    stringstream ss(input);
    ss >> a >> b >> c >> f >> d;
    cout<< setprecision(3)<<fixed;
    cout << a << endl << b << endl <<c << endl<< f << endl ;
    cout<< setprecision(9)<<fixed;
    cout<< d << endl;
    
    return 0;
}

